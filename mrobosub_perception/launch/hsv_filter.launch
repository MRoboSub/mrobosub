<launch>
  <arg name="always_run" default="0" />
  <node 
    pkg="mrobosub_perception"
    type="hsv_filter.py"
    name="hsv_filter_1"
    output="screen"
    args="$(arg always_run)"
  >
    <param name="sub_name" type="str" value="/zed2/zed_node/rgb/image_rect_color" />
    <param name="serv_name" type="str" value="/hsv_buoy_position" />
    <rosparam command="load" file="$(find mrobosub_perception)/params/hsv_filter_1.yaml" />
  </node> 
</launch>
